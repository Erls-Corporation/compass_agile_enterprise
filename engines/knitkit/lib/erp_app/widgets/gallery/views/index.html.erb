<div id="<%=widget_result_id%>"></div>
	
<script type="text/javascript">
	hs.graphicsDir = '../images/high_slide/graphics/';
	hs.showCredits = false;
	hs.align = 'center';
	hs.transitions = ['expand', 'crossfade'];
	hs.outlineType = 'rounded-white';
	hs.fadeInOut = true;
	//hs.dimmingOpacity = 0.75;

	// Add the controlbar
	hs.addSlideshow({
		//slideshowGroup: 'group1',
		interval: 5000,
		repeat: false,
		useControls: true,
		fixedControls: 'fit',
		overlayOptions: {
			opacity: 0.75,
			position: 'bottom center',
			hideOnMouseOut: true
		}
	});
</script>


	<!-- High Slide  -->
	<div class="highslide-gallery">
	<!--
		4) This is how you mark up the thumbnail image with an anchor tag around it.
		The anchor's href attribute defines the URL of the full-size image.
	-->
	<a href="/images/high_slide/gallery1.jpg" class="highslide" onclick="return hs.expand(this)">
		<img src="/images/high_slide/gallery1.thumb.jpg" alt="Highslide JS"
			title="Click to enlarge" />
	</a>

	<!--
		5 (optional). This is how you mark up the caption. The correct class name is important.
	-->

	<div class="highslide-caption">
		Caption for the first image. This caption can be styled using CSS.
	</div>


	<!-- Repetionion of the above -->
	<a href="/images/high_slide/gallery2.jpg" class="highslide" onclick="return hs.expand(this)">
		<img src="/images/high_slide/gallery2.thumb.jpg" alt="Highslide JS"
			title="Click to enlarge" /></a>

	<div class="highslide-caption">
		Caption for the second image.
	</div>

	<a href="/images/high_slide/gallery3.jpg" class="highslide" onclick="return hs.expand(this)">

		<img src="/images/high_slide/gallery3.thumb.jpg" alt="Highslide JS"
			title="Click to enlarge" /></a>

	<div class="highslide-caption">
		Caption for the third image.
	</div>