Compass.ErpApp.<%= container_class_name %>.Applications.<%= application_class_name %>.<%=class_name %>EditableGrid = Ext.extend(Compass.ErpApp.Shared.DynamicEditableGridLoaderPanel, {
    constructor : function(config){
        config = Ext.apply({
            title:'<%=class_name %>',
            xtype:'shared_dynamiceditablegridloaderpanel',
            editable:true,
            setupUrl:'./<%= application_file_name %>/<%=file_name %>/setup',
            dataUrl:'./<%= application_file_name %>/<%=file_name %>/data',
            page:true,
            pageSize:30,
            displayMsg:'Displaying {0} - {1} of {2}',
            emptyMsg:'Empty'
        }, config);
        Compass.ErpApp.<%= container_class_name %>.Applications.<%= application_class_name %>.<%=class_name %>EditableGrid.superclass.constructor.call(this, config);
    }
});

Ext.reg('<%=file_name%>_editablegrid', Compass.ErpApp.<%= container_class_name %>.Applications.<%= application_class_name %>.<%=class_name %>EditableGrid);