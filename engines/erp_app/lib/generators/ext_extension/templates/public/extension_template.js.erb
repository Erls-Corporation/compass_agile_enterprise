Ext.define("<%=javascript_class_name %>",{
  extend:"<%=ext_extension_class %>",
  //uncomment and give an xtype if you want this class to use an xtype
  //alias:'widget.widget_name',
  initComponent: function() {
    <%=javascript_class_name %>.superclass.initComponent.call(this, arguments);
  },
  
  constructor : function(config) {
    config = Ext.apply({}, config);

    <%=javascript_class_name %>.superclass.constructor.call(this, config);
  }
});