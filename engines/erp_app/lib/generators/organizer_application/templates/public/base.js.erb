Ext.ns("Compass.ErpApp.Organizer.Applications.<%= class_name %>");

Compass.ErpApp.Organizer.Applications.<%= class_name %>.Base = function(organizerLayout){

   var menuTreePanel = {
        xtype:'defaultmenutree',
        title:'<%= class_name %>',
        menuRootIconCls:'icon-content',
        rootNodeTitle:'Menu',
        treeConfig:{
            loader:{dataUrl:'./<%= file_name %>/menu'}
        }
    };

    var examplePanel = new Ext.Panel({id:'<%= file_name %>_example_panel', html:'Put your content here', layout:'fit'})

    this.setup = function(){
        organizerLayout.addApplication(menuTreePanel, [examplePanel]);
    };

};

